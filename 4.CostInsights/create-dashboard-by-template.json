{
   "AwsAccountId": "{{AwsAccountId}}",
   "DashboardId": "{{DashboardId}}",
   "SourceEntity":
    { "SourceTemplate": {
       "DataSetReferences": [
         {
           "DataSetPlaceholder": "cur_spice",
           "DataSetArn": "arn:aws:quicksight:{{Region}}:{{AwsAccountId}}:dataset/{{CUR_SPICE_DataSetId}}"
         },
         {
           "DataSetPlaceholder": "cur_direct",
           "DataSetArn": "arn:aws:quicksight:{{Region}}:{{AwsAccountId}}:dataset/{{CUR_DIRECT_DataSetId}}"
         },
         {
           "DataSetPlaceholder": "cur_dimemsions",
           "DataSetArn": "arn:aws:quicksight:{{Region}}:{{AwsAccountId}}:dataset/{{CUR_Dimensions_DataSetId}}"
         }
       ],
       "Arn": "arn:aws:quicksight:{{SourceRegion}}:{{SourceAwsAccountId}}:template/{{TemplateId}}"
     }
    },
    "Permissions": [
        {
            "Principal": "{{QuickSightUser}}",
            "Actions": [
                "quicksight:DescribeDashboard", 
                "quicksight:ListDashboardVersions", 
                "quicksight:UpdateDashboardPermissions", 
                "quicksight:QueryDashboard", 
                "quicksight:UpdateDashboard", 
                "quicksight:DeleteDashboard", 
                "quicksight:UpdateDashboardPublishedVersion", 
                "quicksight:DescribeDashboardPermissions"
            ]
        }
    ]
}